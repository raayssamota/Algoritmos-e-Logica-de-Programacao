programa {
  funcao inicio() {
    //Declaração de Variáveis
    real Celsius,Fahreinheit

    //Entrada de Dados
    escreva("Informe a temperatura em Celsius")
    leia(Celsius)

    //Processamento
    Fahreinheit=(Celsius*9/5)+32

    //Saída de dados 
    escreva("A temperatura em Fahreinheit é", Fahreinheit)
  }
}
